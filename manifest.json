{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Popup Weblio",
	"version": "1",
	"description": "Looks up the selected text on Weblio Dictionary",
	"icons": {
		"128": "weblio.png",
		"48": "weblio.png",
		"16": "weblio.png"
	},

	"browser_action": {
		"default_icon": "weblio.png",
		"default_popup": "popup.html"
	},

	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},

	"permissions": [
		"storage",
		"contextMenus"
	]
}